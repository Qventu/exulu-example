{
  "name": "backendv2",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "engines": {
    "node": "22.17.1"
  },
  "scripts": {
    "dev:server": "tsx watch server.ts",
    "dev:server:otel": "tsx watch --import otel.ts server.ts",
    "dev:worker": "tsx watch worker.ts",
    "dev:worker:otel": "tsx watch --import otel.ts worker.ts",
    "build": "tsup",
    "start:server": "node dist/server.js",
    "start:server:otel": "node --import dist/otel.js dist/server.js",
    "start:worker": "node dist/worker.js",
    "start:worker:otel": "node --import dist/otel.js dist/worker.js",
    "utils:initdb": "tsx init-db.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bun": "latest",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.15.1",
    "husky": "^9.1.7",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/openai": "^2.0.27",
    "@exulu/backend": "^1.24.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "zod": "^3.24.2"
  }
}
