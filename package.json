{
  "name": "backendv2",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "engines": {
    "node": "22.17.1"
  },
  "scripts": {
    "dev:server": "tsx watch server.ts",
    "dev:server:otel": "tsx watch --import otel.ts server.ts",
    "dev:worker": "tsx watch worker.ts",
    "dev:worker:otel": "tsx watch --import otel.ts worker.ts",
    "build": "tsup",
    "start:server": "node dist/server.js",
    "start:server:otel": "node --import dist/otel.js dist/server.js",
    "start:worker": "node dist/worker.js",
    "start:worker:otel": "node --import dist/otel.js dist/worker.js",
    "utils:initdb": "tsx init-db.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bun": "latest",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.15.1",
    "husky": "^9.1.7",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@browserbasehq/sdk": "^2.6.0",
    "@browserbasehq/stagehand": "^2.4.2",
    "@exulu/backend": "^1.25.0",
    "@giphy/js-fetch-api": "^5.6.0",
    "@giphy/react-components": "^10.0.1",
    "@huggingface/transformers": "^3.7.2",
    "@mendable/firecrawl-js": "^3.0.3",
    "@xenova/transformers": "^2.17.2",
    "conventional-changelog-atom": "^5.0.0",
    "JSONStream": "^1.3.5",
    "onnxruntime-node": "^1.22.0-rev",
    "stream-json": "^1.9.1"
  }
}
